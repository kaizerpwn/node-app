                                            <small class="text-muted">dd/mm/yyyy</small></label>
                                        <input type="text" class="form-control date-inputmask" id="date-mask"
                                            placeholder="Datum termina" name="datum_zakazivanja" />
                                    </div>
                                    <div class=" form-group mt-3">
                                        <label>Vrijeme
                                            <small class="text-muted">h/min/am-pm</small></label>
                                        <input type="text" class="form-control date-inputmask" id="date-mask"
                                            name="vrijeme_termina" />
                                    </div>
                                    <div class="form-group row">
                                        <label for="" class="col-md-3 m-t-15">Savjetnik</label>
                                        <div class="col-md-9">
                                            <select name="savjetnik" class="form-control">
                                                <% for(let i=0; i < savjetnici.length; i++) { %>
                                                    <option
                                                        value="<%= savjetnici[i].first_name +" "+savjetnici[i].last_name %>">
                                                        <%= savjetnici[i].first_name +" "+savjetnici[i].last_name %></option>
                                                <% } %>
                                            </select>
                                        </div>
                                    </div>
                                    <div class=" form-group row">
                                                            <label for="" class="col-md-3 m-t-15">Mjesto rada</label>
                                                            <div class="col-md-9">
                                                                <select name="grad" class="form-control">
                                                                    <% for(let i=0; i<gradovi.length; i++) { %>
                                                                        <option value="<%= gradovi[i].name %>">
                                                                            <%= gradovi[i].name %>
                                                                        </option>
                                                                        <% } %>
                                                                </select>
                                                            </div>
                                        </div>
                                    </div>
                                </div>
